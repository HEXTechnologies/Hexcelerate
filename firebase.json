{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/Admin",
        "destination": "/Admin.html"
      },
      {
        "source": "/Dashboard",
        "destination": "/Dashboard.html"
      },
      {
        "source": "/Project",
        "destination": "/Project.html"
      },
      {
        "source": "/Project-Management",
        "destination": "/Project-Management.html"
      },
      {
        "source": "/security-report",
        "destination": "/security-report.html"
      },
      {
        "source": "/Waitlist",
        "destination": "/Waitlist.html"
      },
      {
        "source": "/Categories/community",
        "destination": "/Categories/community.html"
      },
      {
        "source": "/Categories/transportation",
        "destination": "/Categories/transportation.html"
      },
      {
        "source": "/Categories/school",
        "destination": "/Categories/school.html"
      },
      {
        "source": "/Categories/employment",
        "destination": "/Categories/employment.html"
      },
      {
        "source": "/Categories/publicSafety",
        "destination": "/Categories/publicSafety.html"
      },
      {
        "source": "/CandidatesProfile",
        "destination": "/CandidatesProfile.html"
      },
      {
        "source": "/CompaniesProfile",
        "destination": "/CompaniesProfile.html"
      },
      {
        "source": "/Candidates/:id",
        "destination": "/Candidates/[id].html"
      },
      {
        "source": "/Companies/:id",
        "destination": "/Companies/[id].html"
      },
      {
        "source": "/Companies",
        "destination": "/Companies.html"
      },
      {
        "source": "/Candidates",
        "destination": "/Candidates.html"
      },
      {
        "source": "SignIn",
        "destination": "/SignIn.html"
      },
      {
        "source": "SignUp",
        "destination": "/SignUp.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "database": {
    "rules": "database.rules.json"
  }
}